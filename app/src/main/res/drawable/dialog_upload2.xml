<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <!--无内部填充颜色-->
            <solid android:color="@android:color/transparent" />
            <stroke
                android:width="1dp"
                android:color="@color/alltask_background" />
            <corners android:radius="100dp" />
            <padding
                android:left="10dp"
                android:right="10dp"
                android:top="7dp"
                android:bottom="7dp"/>
        </shape>
    </item>
</layer-list>